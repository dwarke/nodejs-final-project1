<%- include('header') %>

    <section class="page-heading wow fadeIn" data-wow-duration="1.5s"
        style="background-image: url(../../files/images/01-heading.jpg)">
        <div class="container">
            <div class="page-name">
                <h1>Latest Course</h1>
                <span>Lovely layout of heading</span>
            </div>
        </div>
    </section>

    <section class="portfolio on-portfolio">
        <div class="container">
            <div class="col-sm-12 text-center">
                <div id="projects-filter">
                    <a href="#" data-filter="*" class="active">Show All</a>
                    <a href="#" data-filter=".furniture">Furniture</a>
                </div>
            </div>
            <div class="row">
                <div class="row" id="portfolio-grid">
                    <% user.map((val)=>{
                        if(val.status === '1'){ %>
                        <div class="item col-md-4 col-sm-6 col-xs-12 furniture">
                            <figure>
                                <img alt="1-image" src="/<%= val.images %>" style="width: 100%; height: 300px;">
                                <figcaption>
                                    <h3><%=val.title1 %></h3>
                                    <p><%= val.title2 %></p>
                                </figcaption>
                            </figure>
                        </div>
                        <% } }) %>
                </div>
            </div>
            <div class="col-md-12">
                <div class="portfolio-page-nav text-center">
                    <ul>
                        <li><a href="#" class="current">1</a></li>
                        <li><a href="#">2</a></li>
                        <li><a href="#">3</a></li>
                        <li><a href="#"><i class="fa fa-angle-double-right"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </section>

    <%- include('footer') %>